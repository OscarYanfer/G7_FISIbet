type Mutation{
    guardarEvento(createEventRequest:CreateEventRequest):EventModel,
}
type Query{
    listarEventos : [EventModel]
}

type EventModel{
    id : String,
    name : String,
    description : String,
    status : Int,
    registered: String,
    updated: String
}

input CreateEventRequest{
    name: String,
    description: String
}
